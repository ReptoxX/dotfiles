# --- START CODER JETBRAINS coder.reptoxx.com
Host coder-jetbrains--mycollection.main--coder.reptoxx.com
  HostName coder.mycollection.main
  ProxyCommand /home/reptoxx/.local/share/coder-gateway/coder.reptoxx.com/coder-linux-amd64 --global-config /home/reptoxx/.local/share/coder-gateway/coder.reptoxx.com/config ssh --stdio mycollection.main
  ConnectTimeout 0
  StrictHostKeyChecking no
  UserKnownHostsFile /dev/null
  LogLevel ERROR
  SetEnv CODER_SSH_SESSION_TYPE=JetBrains
# --- END CODER JETBRAINS coder.reptoxx.com
